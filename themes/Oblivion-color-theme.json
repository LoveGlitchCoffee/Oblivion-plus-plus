				"keyword.control.directive.conditional.c",
				"keyword.control.directive.conditional.cpp"
			],
			"settings": {
				"foreground": "#ef2929",
				"fontStyle": "bold"
			}
		},
		{
			"name": "namespaces",
			"scope": [
				"meta.namespace-block.cpp",
				"entity.scope.name.cpp"
			],
			"settings": {
				"foreground": "#ef2929",
			}
		},
		{
			"name": "type names",
			"scope": [
				"entity.name.type.cpp",
