{
	"name": "Oblivion",
	"tokenColors": [
		{
			"name": "Directives (include, ifdef)",
			"scope": [
				"punctuation.definition.directive",
				"keyword.control.directive"
			],
			"settings": {
				"foreground": "#ef2929",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Scope Resolution",
			"scope": [
				"entity.name.type.namespace.scope-resolution",
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#ef2929",
			}
		},
		{
			"name": "Namespaces",
			"scope": [
				"entity.name.namespace",
				"meta.body.namespace.cpp",
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#8ae234",
			}
		},
		{
			"name": "Type Names",
			"scope": [
				"entity.name.type",
				"storage.type.primitive",
				"storage.type.user-defined",
				"meta.function.definition.parameters.cpp",
				"storage.type",
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#8ae234",
			}
		},
		{
			"name": "Storage type",
			"scope": [
				"storage.type.class",
				"storage.type.template.cpp",
				"storage.type.template.argument.class",
				"storage.type.namespace.definition.cpp"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#ad7fa8",
			}
		},
		{
			"name": "Comment",
			"scope": [
				"comment",
				 "punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#888a85"
			}
		},
		{
			"name": "String",
			"scope": [
				"string",
				"punctuation.definition.string"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#fce94f"
			}
		},
		{
			"name": "Operators",
			"scope": [
				"punctuation.section",
				"punctuation.definition",
				"punctuation.separator",
				"punctuation.terminator",
				"meta.function.definition.special.constructor",
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#eeeeec"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"meta.body.function.definition",
				"meta.block",
				"variable.parameter.cpp"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#eeeeec"
			}
		},
		{
			"name": "Other Constants (numeric and char)",
			"scope": [
				"constant.character",
				"constant.other",
				"constant.numeric.decimal.cpp"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#ce5c00"
			}
		},
		{
			"name": "Functions",
			"scope": [
				"entity.name.function"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#729fcf",
			}
		},
		{
			"name": "Access members",
			"scope": [
				"meta.bracket.square.access.cpp",
				"variable.other.object.access.cpp", // works better as this colour, asumming only pass names to functions
				"variable.other.property",
				"variable.other.object.property.cpp"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#8ae234"
			}
		},
		{
			"name": "User-defined values (enum values) ",
			"scope": [
				"meta.conditional.case.cpp"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#8ae234"
			}
		},
		{
			"name": "Keyword",
			"scope": [
				"keyword"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#ad7fa8"
			}
		},
		{
			"name": "Scope Qualifiers",
			"scope": [
				"storage.modifier",
				"storage.type.modifier.access.control.public.cpp",
				"storage.type.modifier.access.control.private.cpp",
				"storage.type.modifier.access.control.protected.cpp"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#ad7fa8"
			}
		},
		{
			"name": "Constants",
			"scope": [
				"constant.language.c",
				"constant.language.cpp",
				"constant.language.true.cpp",
				"constant.language.false.cpp"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#ef2929"
			}
		},
		{
			"name": "Inheritance Qualifier",
			"scope":
			[
				"entity.name.type.inherited"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#ad7fa8"
			}
		},
		{
			"name": "Invalid",
			"scope": "invalid",
			"settings": {
				"fontStyle": "",
				"foreground": "#ef2929"
			}
		},
		{
			"name": "Invalid deprecated",
			"scope": "invalid.deprecated",
			"settings": {
				"foreground": "#ef2929"
			}
		},
		{
			"name": "diff: header",
			"scope": [
				"meta.diff",
				"meta.diff.header"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#babdb6"
			}
		},
		{
			"name": "diff: deleted",
			"scope": "markup.deleted",
			"settings": {
				"fontStyle": "",
				"foreground": "#a40000"
			}
		},
		{
			"name": "diff: changed",
			"scope": "markup.changed",
			"settings": {
				"fontStyle": "",
				"foreground": "#f57900"
			}
		},
		{
			"name": "diff: inserted",
			"scope": "markup.inserted",
			"settings": {
				"foreground": "#8ae234"
			}
		},
		{
			"name": "Markup Quote",
			"scope": "markup.quote",
			"settings": {
				"foreground": "#729fcf"
			}
		},
		{
			"name": "Markup Styling",
			"scope": [
				"markup.bold",
				"markup.italic"
			],
			"settings": {
				"foreground": "#729fcf"
			}
		},
		{
			"name": "Markup Inline",
			"scope": "markup.inline.raw",
			"settings": {
				"fontStyle": "",
				"foreground": "#c4a000"
			}
		},
		{
			"name": "Markup Setext Header",
			"scope": "markup.heading.setext",
			"settings": {
				"fontStyle": "",
				"foreground": "#edd400"
			}
		}
	],
